{
  "name": "orchidify-api",
  "version": "1.0.0",
  "description": "Orchidify API",
  "author": "Deco-Team",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org deco-team-capstone --project node-nestjs ./dist && sentry-cli sourcemaps upload --org deco-team-capstone --project node-nestjs ./dist"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.3",
    "@payos/node": "^1.0.6",
    "@sentry/cli": "^2.31.2",
    "@sentry/node": "^7.113.0",
    "@sentry/profiling-node": "^7.113.0",
    "@types/moment": "^2.13.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.4.0",
    "discord.js": "^14.15.1",
    "ejs": "^3.1.9",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.1.0",
    "mongoose-paginate-v2": "^1.8.0",
    "mongoose-slug-updater": "^3.3.0",
    "nest-commander": "^3.12.5",
    "nestjs-cls": "^3.6.0",
    "nestjs-i18n": "^10.4.0",
    "nodemailer": "^6.9.9",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/through": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "preset": "ts-jest",
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "rootDir": "src",
    "testMatch": [
      "<rootDir>/**/*.e2e-spec.ts",
      "<rootDir>/**/test/*.e2e-spec.ts",
      "<rootDir>/**/*.spec.ts",
      "<rootDir>/**/test/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/seeds",
      ".schema.ts",
      ".pipe.ts",
      ".guard.ts",
      ".dto.ts",
      ".validator.ts"
    ],
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70
      }
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@media/(.*)$": "<rootDir>/media/$1",
      "^@mail/(.*)$": "<rootDir>/mail/$1",
      "^@auth/(.*)$": "<rootDir>/auth/$1",
      "^@learner/(.*)$": "<rootDir>/learner/$1",
      "^@instructor/(.*)$": "<rootDir>/instructor/$1",
      "^@staff/(.*)$": "<rootDir>/staff/$1",
      "^@garden-manager/(.*)$": "<rootDir>/garden-manager/$1",
      "^@garden/(.*)$": "<rootDir>/garden/$1",
      "^@recruitment/(.*)$": "<rootDir>/recruitment/$1",
      "^@seeds/(.*)$": "<rootDir>/seeds/$1",
      "^@i18n/(.*)$": "<rootDir>/i18n/$1",
      "^@payment/(.*)$": "<rootDir>/payment/$1",
      "^@review/(.*)$": "<rootDir>/review/$1",
      "^@setting/(.*)$": "<rootDir>/setting/$1",
      "^@course/(.*)$": "<rootDir>/course/$1",
      "^@class/(.*)$": "<rootDir>/class/$1",
      "^@garden-timesheet/(.*)$": "<rootDir>/garden-timesheet/$1",
      "^@src/(.*)$": "<rootDir>/$1"
    }
  }
}
